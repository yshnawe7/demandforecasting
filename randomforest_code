#RANDOM FOREST REGRESSION
from sklearn.ensemble import RandomForest Regressor
md1 RandomForest Regressor(n_estimators-100, max_depth-6)
mdl.fit(X, Y)
pred mdl.predict([[200.06]])
print("Predicted value (RFR): ",pred[0])
print("Accuracy (RFR): ",mdl.score (X[:100], Y[:100])*100)
plt.scatter (X['Net orders'], Y, color-'b')
plt.plot(X['Net orders'], mdl.predict(X), color-'black', linewidth=3)
plt.xlabel('Net orders')
plt.ylabel('Demand')
plt.show()
from sklearn.ensemble import RandomForest Regressor
from sklearn.metrics import mean_absolute_error, mean_squared_error, r2_score rf_model-Random Forest Regressor(n_estimators-100, random_state-42)
rf_model.fit(x_train, y_train)
y_pred-rf_model.predict(x_test)
mse-mean_squared_error(y_test,y_pred)
r2-r2_score(y_test,y_pred)
mae-mean_absolute_error(y_test,y_pred)
print("Mean Square Error of RandomForest Regression", mse)
print("R2 score of Random Forest Regression", r2)
print("Mean absolute Error of RandomForest Regression", mae)
